<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev3.view.Main">
   <top>
      <VBox prefHeight="50.0" prefWidth="370.0" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="QuestionDetailsTitle" text="Question Title">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="QuestinoDetailsQuestionText" text="Question text">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <Separator prefWidth="200.0" />
         </children>
         <BorderPane.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </BorderPane.margin>
      </VBox>
   </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <ListView fx:id="QuestionDetailsAnswers" prefHeight="250.0" prefWidth="380.0" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </VBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="50.0" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#goHome" prefWidth="120.0" text="Home">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button mnemonicParsing="false" onMouseClicked="#goToAddAnswer" prefWidth="120.0" text="Add Answer">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button mnemonicParsing="false" onMouseClicked="#goToAddFeedback" prefWidth="120.0" text="Add Feedback">
               <font>
                  <Font size="14.0" />
               </font></Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
