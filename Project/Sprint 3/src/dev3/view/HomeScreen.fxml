<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev3.view.Main">
   <top>
      <BorderPane prefHeight="46.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <right>
            <Button mnemonicParsing="false" onMouseClicked="#goToLogIn" text="Log out" BorderPane.alignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font></Button>
         </right>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
         <left>
            <Label fx:id="HomeScreenScreenName" text="Home Screen" BorderPane.alignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </left>
      </BorderPane>
   </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="150.0" prefWidth="400.0" spacing="20.0">
               <children>
                  <Button mnemonicParsing="false" onMouseClicked="#goToCreateGroup" prefHeight="100.0" prefWidth="150.0" text="Create Group">
                     <font>
                        <Font size="14.0" />
                     </font></Button>
                  <Button mnemonicParsing="false" onMouseClicked="#goToJoinGroup" prefHeight="100.0" prefWidth="150.0" text="Find Groups">
                     <font>
                        <Font size="14.0" />
                     </font></Button>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </HBox>
            <HBox alignment="CENTER" prefHeight="150.0" prefWidth="400.0" spacing="20.0">
               <children>
                  <Button mnemonicParsing="false" onMouseClicked="#goToAddQuestion" prefHeight="100.0" prefWidth="150.0" text="Ask Question ">
                     <font>
                        <Font size="14.0" />
                     </font></Button>
                  <Button mnemonicParsing="false" onMouseClicked="#goToSearchResults" prefHeight="100.0" prefWidth="150.0" text="Search Question">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
