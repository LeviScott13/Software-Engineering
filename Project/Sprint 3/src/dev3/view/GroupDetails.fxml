<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev3.view.Main">
   <top>
      <BorderPane prefHeight="78.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <center>
            <Label fx:id="GroupDetailsGroupTitle" text="Label" BorderPane.alignment="CENTER">
               <font>
                  <Font size="36.0" />
               </font></Label>
         </center>
      </BorderPane>
   </top>
   <center>
      <BorderPane prefHeight="323.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <top>
            <Label fx:id="GroupDetailsGroupDescription" text="Label" BorderPane.alignment="CENTER" />
         </top>
         <bottom>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="25.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onMouseClicked="#goHome" prefWidth="150.0" text="Cancel">
                     <font>
                        <Font size="14.0" />
                     </font></Button>
                  <Button mnemonicParsing="false" onMouseClicked="#joinGroupOnClick" prefWidth="150.0" text="Join Group">
                     <font>
                        <Font size="14.0" />
                     </font></Button>
               </children>
            </HBox>
         </bottom>
         <center>
            <VBox spacing="10.0">
               <children>
                  <Label text="Top 10 Most Active Members:" />
                  <ListView fx:id="GroupDetailsMemberList" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                     <BorderPane.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </BorderPane.margin>
                  </ListView>
               </children>
               <BorderPane.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </BorderPane.margin>
            </VBox>
         </center>
      </BorderPane>
   </center>
</BorderPane>
